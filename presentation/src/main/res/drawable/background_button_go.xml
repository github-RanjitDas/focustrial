<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="125dp"
        android:height="125dp"
        android:gravity="center">

        <shape
            android:shape="oval"
            android:useLevel="false">

            <stroke
                android:width="2dp"
                android:color="#5FBB46" />
            <solid android:color="#184C89" />

        </shape>
    </item>
    <item
        android:height="141dp"
        android:width="141dp"
        android:gravity="center">

        <shape
            android:shape="oval"
            android:useLevel="false">

            <stroke
                android:width="8dp"
                android:color="@color/whiteTransparent" />

        </shape>
    </item>
</layer-list>